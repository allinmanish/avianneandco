<config>
    <modules>
        <Mango_Jqueryfiles>
            <version>0.1.0</version>
        </Mango_Jqueryfiles>
    </modules>
    <frontend>
        <routers>
            <jqueryfiles>
                <use>standard</use>
                <args>
                    <module>Mango_Jqueryfiles</module>
                    <frontName>jqueryfiles</frontName>
                </args>
            </jqueryfiles>
        </routers>
        <layout>
            <updates>
                <jqueryfiles>
                    <file>mango_jqueryfiles.xml</file>
                </jqueryfiles>
            </updates>
        </layout>
    </frontend>

</config>	
