<config>
    <modules>
        <Mango_Frontendupdates>
            <version>0.1.0</version>
        </Mango_Frontendupdates>
    </modules>
    <frontend>
        <routers>
            <frontendupdates>
                <use>standard</use>
                <args>
                    <module>Mango_Frontendupdates</module>
                    <frontName>frontendupdates</frontName>
                </args>
            </frontendupdates>
        </routers>
        <layout>
            <updates>
                <frontendupdates>
                    <file>frontendupdates.xml</file>
                </frontendupdates>
            </updates>
        </layout>
    </frontend>

</config>	
