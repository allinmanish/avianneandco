<config>
    <modules>
        <Amazon_Diagnostics>
            <version>1.1.0</version>
        </Amazon_Diagnostics>
    </modules>
    <global>
        <helpers>
            <amazon_diagnostics>
                <class>Amazon_Diagnostics_Helper</class>
            </amazon_diagnostics>
        </helpers>
        <blocks>
            <amazon_diagnostics>
                <class>Amazon_Diagnostics_Block</class>
            </amazon_diagnostics>
        </blocks>
    </global>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <amazon_diagnostics after="Mage_Adminhtml">Amazon_Diagnostics</amazon_diagnostics>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
    <adminhtml>
        <translate>
            <modules>
                <Amazon_Diagnostics>
                    <files>
                        <default>Amazon_Diagnostics.csv</default>
                    </files>
                </Amazon_Diagnostics>
            </modules>
        </translate>
    </adminhtml>
</config>
