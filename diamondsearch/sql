SELECT  * FROM  dev_products where price > 0  AND price >= 290 AND price <= 1000000 AND carat >= 0.11 AND carat <= 9.99 AND cut in ('Good','Very Good','Ideal','Premium') AND color in ('D','E','F','G','H','I') AND clarity in ('IF','VVS1','VVS2','VS1','VS2','SI1','SI2') AND Flour in ('NO','FB','MB','MED','ST BLUE','VST BLUE') AND Polish in ('ID','EX','VG','GD','F') AND Sym in ('ID','EX','VG','GD','F') ORDER BY price asc LIMIT 0, 40